  "license": "MIT",
  "homepage": "http://github.com/eduardomrb/slim-middleware-crossorigin",
  "authors": [
  {
    "name": "Eduardo Borges",
    "email": "eduardomrb@gmail.com"
  }
  ],

    "minimum-stability": "dev",

    "require": {
      "php": ">=5.3.0",
      "slim/slim": "dev-master"
    },

    "require-dev": {
